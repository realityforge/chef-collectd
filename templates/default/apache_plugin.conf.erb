LoadPlugin "<%= @type %>"

<% if not @config.empty? %>
<Plugin "<%= @type %>">
<% @config.sort{|a,b| a[0].to_s <=> b[0].to_s}.map do |instance, instance_conf| %>
  <Instance "<%= instance %>">
  <% if not instance_conf.empty? %>
    <% instance_conf.sort{|a,b| a[0].to_s <=> b[0].to_s}.map do |key, value|
      if value.is_a? Array
        value.each do |subvalue| %>
    <%= key %> <%= collectd_option(subvalue) %>
    <% end else %>
    <%= key %> <%= collectd_option(value) %>
    <% end end %>
  <% end %>
  </Instance>
<% end %>
</Plugin>
<% end %>
