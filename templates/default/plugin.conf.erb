LoadPlugin "<%= @type %>"

<% if not @config.empty? %>
<Plugin "<%= @type %>">
  <% @config.sort{|a,b| a[0].to_s <=> b[0].to_s}.map do |key, value|
    if value.is_a? Array
      value.each do |subvalue| %>
  <%= collectd_key(key) %> <%= collectd_option(subvalue) %>
  <% end else %>
  <%= collectd_key(key) %> <%= collectd_option(value) %>
  <% end end %>
</Plugin>
<% end %>
