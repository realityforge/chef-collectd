LoadPlugin "<%= @type %>"

<% if not @config.empty? %>
<Plugin "<%= @type %>">
<% @config.sort{|a,b| a[0].to_s <=> b[0].to_s}.map do |instance_name, instance_info| %>
  <%= "<Instance \"#{instance_name}\">" %>
  <% if not instance_info.empty? %>
    <% instance_info.sort{|a,b| a[0].to_s <=> b[0].to_s}.map do |key, value|
      if value.is_a? Array
        value.each do |subvalue| %>
    <%= key %> <%= collectd_option(subvalue) %>
    <% end else %>
    <%= key %> <%= collectd_option(value) %>
    <% end end %>
  <% end %>
  <%= "</Instance>" %>
<% end %>
</Plugin>
<% end %>
